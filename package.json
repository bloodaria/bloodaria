{
    "name": "Bloodaria",
    "preductname": "Bloodaria Launcher",
    "version": "1.0.2",
    "description": "Launcher Bloodaria",
    "main": "src/app.js",
    "url": "http://launcher.rizyox.fr",
    "scripts": {
        "start": "cross-env-shell NODE_ENV=dev electron .",
        "dev": "cross-env-shell DEV_TOOL=open nodemon --exec npm start",
        "build": "node build.js --obf=true --build=platform",
        "icon": "node build.js --icon=https://github.com/bloodaria/bloodaria/blob/master/src/assets/images/icon.png"
    },
    "license": "CCANC",
    "nodemonConfig": {
        "ext": "js,html,css",
        "ignore": "**/test"
    },
    "dependencies": {
        "discord-rpc": "^4.0.1",
        "electron-updater": "^6.1.8",
        "minecraft-java-core": "^4.1.1",
        "node-bdd": "^2.0.4",
        "node-fetch": "^2.7.0",
        "xml-js": "^1.6.11"
    },
    "devDependencies": {
        "cross-env": "^7.0.3",
        "electron": "^36.2.1",
        "electron-builder": "^24.13.3",
        "javascript-obfuscator": "^4.1.0",
        "jimp": "^0.22.12",
        "nodemon": "^3.1.2",
        "png2icons": "^2.0.1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bloodaria/bloodaria.git"
    }
}
